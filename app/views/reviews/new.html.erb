<h1>Add a review for <%= @restaurant.name %></h1>

<%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.error_notification %>
<div class="form-inputs">
<%= f.input :content %>

 <%= f.input :rating, collection: 0..5 %>

</div>
<div class="form-actions">
<%= f.button :submit %>
 </div>
<% end %>
<%= link_to 'Back', restaurants_path %>




